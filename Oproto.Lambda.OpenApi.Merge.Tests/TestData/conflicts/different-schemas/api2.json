{
  "openapi": "3.0.1",
  "info": {
    "title": "API 2 with Different Schema",
    "version": "1.0.0"
  },
  "paths": {
    "/api2/entities": {
      "get": {
        "operationId": "getApi2Entities",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Entity"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Entity": {
        "type": "object",
        "properties": {
          "entityId": {
            "type": "integer"
          },
          "title": {
            "type": "string"
          },
          "description": {
            "type": "string"
          }
        },
        "required": ["entityId", "title"]
      }
    }
  }
}
